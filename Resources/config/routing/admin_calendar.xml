<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="soloist_calendar_admin_calendar_index" pattern="/">
        <default key="_controller">SoloistCalendarBundle:AdminCalendar:index</default>
    </route>

    <route id="soloist_calendar_admin_calendar_new" pattern="/new">
        <default key="_controller">SoloistCalendarBundle:AdminCalendar:new</default>
    </route>

    <route id="soloist_calendar_admin_calendar_create" pattern="/create">
        <default key="_controller">SoloistCalendarBundle:AdminCalendar:create</default>
    </route>

    <route id="soloist_calendar_admin_calendar_edit" pattern="/edit/{id}">
        <default key="_controller">SoloistCalendarBundle:AdminCalendar:edit</default>
    </route>

    <route id="soloist_calendar_admin_calendar_update" pattern="/update/{id}">
        <default key="_controller">SoloistCalendarBundle:AdminCalendar:update</default>
    </route>

    <route id="soloist_calendar_admin_calendar_delete" pattern="/delete/{id}">
        <default key="_controller">SoloistCalendarBundle:AdminCalendar:delete</default>
    </route>

    <route id="soloist_calendar_admin_event_by_calendar" pattern="/show/by-calendar/{id}">
        <default key="_controller">SoloistCalendarBundle:AdminCalendar:showEvents</default>
    </route>

</routes>
